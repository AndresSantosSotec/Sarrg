{
  "expo": {
    "name": "coosanjerfit",
    "slug": "coosanjerfit",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_fit_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prolink.coosanjerfit",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["motion"],
        "NSMotionUsageDescription": "Esta app necesita acceso a los sensores de movimiento para contar tus pasos y medir tu actividad física.",
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara para dejar constancia de la actividad física y realizar otras funciones relacionadas con la cámara."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_fit_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.sargefitapp",
      "permissions": [
        "com.google.android.gms.permission.ACTIVITY_RECOGNITION",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/logo_fit_icon.png"
    },
    "plugins": [
      [
        "expo-sensors",
        {
          "motionPermission": "Permitir que $(PRODUCT_NAME) acceda a los datos de movimiento y fitness."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2644c7b8-c85a-4761-b183-9bf9e89e2ae8"
      }
    },
    "owner": "ebsoltech"
  }
}
